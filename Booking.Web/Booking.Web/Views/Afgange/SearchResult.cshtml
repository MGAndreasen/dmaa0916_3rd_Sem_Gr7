
@{
    ViewBag.Title = "SearchResult";
}

<h1>SearchResult</h1>




@{
    try
    {
        var client = ViewBag.proxy;
        var depart = client.GetAllDepartures();

        foreach(var d in depart)
        {
            <p>@d.Id </p>
        }



    }

    catch(Exception)
    {
           <p>NAH NIGGA NAH</p>
    }
    
}








