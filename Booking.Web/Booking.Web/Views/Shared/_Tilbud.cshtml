@using Booking.Web.Helpers

<div style="width: 770px; height: inherit;">
    <div class="">
        <p>Rejser!</p>
        <h1>Tilbuds Rejser</h1>
    </div>
    @{
        try
        {
            var client = ViewBag.proxy;
            var dest = client.GetAllDestinations();

            foreach (var s in dest)
            {
                <div class="Boxed floatLeft">
                    <h1>@s.NameDestination</h1>
                    <h3>Fra Aalborg</h3>
                    <h3>Fra/enkeltbillet</h3>
                </div>
            }
        }
        catch (Exception)
        {
            <p>Der er sket en fejl!</p>
        }
       
    }
</div>



